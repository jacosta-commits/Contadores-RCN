{
  "name": "contadores-web",
  "version": "1.0.0",
  "private": true,
  "description": "ZENTRIK · RCN Contadores (server + workers + web)",
  "main": "server/server.js",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "node server/server.js",
    "dev": "nodemon --watch server --watch workers --ext js --exec node server/server.js",
    "poller": "node workers/poller/index.js",
    "snapshots": "node workers/snapshots/index.js",
    "alerts": "node workers/alerts/index.js",
    "pm2:start": "pm2 start pm2/ecosystem.contadores.js",
    "pm2:reload": "pm2 reload pm2/ecosystem.contadores.js",
    "pm2:logs": "pm2 logs --lines 300",
    "test": "echo \"No hay tests aún\" && exit 0"
  },
  "keywords": [
    "ZENTRIK",
    "RCN",
    "contadores",
    "modbus",
    "socket.io",
    "mssql"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^8.1.0",
    "modbus-serial": "^8.0.23",
    "mssql": "^10.0.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
